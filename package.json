{
    "name": "dye_backend",
    "version": "1.0.0",
    "main": "app.ts",
    "repository": "https://github.com/nfl1ryxditimo12/DYE_Backend.git",
    "author": "nfl1ryxditimo12 <nfl1ryxditimo12@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build-ts": "cross-env NODE_ENV=production webpack --config webpack.config.js",
        "build-test-ts": "cross-env NODE_ENV=test webpack --config webpack.config.js",
        "build-dev-ts": "yarn run cross-env NODE_ENV=development webpack --config webpack.config.js",
        "prestart": "tsc",
        "serve": "cross-env NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register --files ./src/app.ts",
        "test": "npm run serve",
        "start": "yarn run serve"
    },
    "dependencies": {
        "@types/cors": "^2.8.12",
        "axios": "^0.24.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "morgan": "^1.10.0",
        "mysql2": "^2.3.3",
        "sequelize": "^6.12.0-alpha.1",
        "webpack": "^5.64.4"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.11.10",
        "@types/validator": "^13.7.0",
        "cross-env": "^7.0.3",
        "nodemon": "^2.0.15",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.12.0",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "typescript": "^4.5.2",
        "webpack-cli": "^4.9.1",
        "webpack-node-externals": "^3.0.0"
    },
    "_moduleAlias": {
        "@routes/*": "./dist/routes/*",
        "@modules/*": "./dist/modules/*",
        "@controllers/*": "./dist/controllers/*",
        "@models/*": "./dist/models/*",
        "@configs/*": "./dist/configs/*"
    }
}
