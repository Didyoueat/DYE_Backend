{
    "name": "dye_backend",
    "version": "1.0.0",
    "main": "app.ts",
    "repository": "https://github.com/nfl1ryxditimo12/DYE_Backend.git",
    "author": "nfl1ryxditimo12 <nfl1ryxditimo12@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build-ts": "cross-env NODE_ENV=production webpack --config webpack.config.js",
        "build-test-ts": "cross-env NODE_ENV=test webpack --config webpack.config.js",
        "build-dev-ts": "cross-env NODE_ENV=development webpack-dev-server",
        "prestart": "tsc",
        "serve-prod": "cross-env NODE_ENV=production nodemon --exec ts-node -r tsconfig-paths/register --files ./src/app.ts",
        "serve-test": "cross-env NODE_OPTIONS=--max-old-space-size=2048 NODE_ENV=test ts-node -r tsconfig-paths/register --files ./src/app.ts &",
        "serve-dev": "cross-env NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register --files ./src/app.ts",
        "start": "yarn run serve-prod",
        "test": "yarn run serve-test",
        "dev": "yarn run serve-dev",
        "jest": "cross-env NODE_ENV=jest jest --setupFiles dotenv/config --config jest.config.js --detectOpenHandles --forceExit"
    },
    "dependencies": {
        "@types/cors": "^2.8.12",
        "aws-sdk": "^2.1046.0",
        "axios": "^0.24.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.4",
        "multer-s3": "^2.10.0",
        "mysql2": "^2.3.3",
        "redis": "^4.0.1",
        "reflect-metadata": "^0.1.13",
        "swagger-cli": "^4.0.4",
        "swagger-ui-express": "^4.3.0",
        "typeorm": "^0.2.41",
        "webpack": "^5.64.4",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.5",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/multer-s3": "^2.7.11",
        "@types/node": "^16.11.10",
        "@types/redis": "^4.0.11",
        "@types/supertest": "^2.0.11",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/validator": "^13.7.0",
        "@types/yamljs": "^0.2.31",
        "cross-env": "^7.0.3",
        "http-status": "^1.5.0",
        "jest": "^27.4.5",
        "jest-mock-req-res": "^1.0.2",
        "nodemon": "^2.0.15",
        "supertest": "^6.1.6",
        "ts-jest": "^27.1.2",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.12.0",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "typescript": "^4.5.2",
        "webpack-cli": "^4.9.1",
        "webpack-node-externals": "^3.0.0"
    },
    "_moduleAlias": {
        "@middlewares/*": "./dist/middlewares/*",
        "@routes/*": "./dist/routes/*",
        "@controllers/*": "./dist/controllers/*",
        "@services/*": "./dist/services/*",
        "@repository/*": "./dish/repository/*",
        "@entities/*": "./dist/entities/*",
        "@modules/*": "./dist/modules/*"
    }
}
